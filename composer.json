{
  "name": "linqinp/linqinp",
  "description": "linqinp is for using something like C# LINQ in php.",
  "keywords": [
    "linq",
    "C#"
  ],
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "taito",
      "email": "hanshin.abingdon@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "phpunit/php-code-coverage": "^9.2"
  },
  "autoload": {
    "psr-4": {
      "Linqinp\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LinqinpTest\\": "test"
    }
  },
  "scripts": {
    "phpunit": "php vendor/phpunit/phpunit/phpunit --coverage-html coverage",
    "coverag": "phpdbg -qrr vendor/phpunit/phpunit/phpunit test/ --coverage-html coverag"
  }
}
